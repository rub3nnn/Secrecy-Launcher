{
  "name": "SLauncher",
  "version": "1.6.3",
  "description": "All in one Minecraft Launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-installer": "electron-packager . --overwrite --icon=./files/default/icon.ico",
    "build": "electron-builder",
    "make": "electron-forge make"
  },
  "build": {
    "appId": "SLauncher",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./files/default/s1.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "./files/default/s1.ico",
      "uninstallDisplayName": "Desinstalar SLauncher",
      "allowToChangeInstallationDirectory": false,
      "runAfterFinish": true,
      "language": "0x000A"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsRubenete/secrecylauncher.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "SECRECY NETWORK",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^17.0.1",
    "electron-builder": "^23.0.3"
  },
  "dependencies": {
    "@dogstudio/highway": "^2.2.1",
    "@xmcl/asm": "^0.1.4",
    "@xmcl/mod-parser": "^3.3.0",
    "@xmcl/server-info": "^2.0.4",
    "@xmcl/system": "^2.2.3",
    "archiver": "^5.3.1",
    "auto-launch": "^5.0.5",
    "bee-mod-parser": "^1.2.2",
    "bootstrap": "^5.1.3",
    "child_process": "^1.0.2",
    "directory-tree": "^3.3.0",
    "discord-rich-presence": "^0.0.8",
    "electron-dl": "^3.3.0",
    "electron-open-link-in-browser": "^1.0.2",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.0.2",
    "express": "^4.18.1",
    "gsap": "^3.10.4",
    "i": "^0.3.7",
    "jquery": "^3.6.0",
    "jsonfile": "^6.1.0",
    "jszip": "^3.10.0",
    "minecraft-launcher-core": "^3.16.17",
    "minecraft-server-parser": "^1.0.0",
    "mods": "^0.9.0",
    "msmc": "^3.1.3",
    "mysql": "^2.18.1",
    "net": "^1.0.2",
    "node-downloader-helper": "^2.1.1",
    "node-fetch": "^2.6.7",
    "node-notifier": "^10.0.1",
    "node-stream-zip": "^1.15.0",
    "npm": "^8.14.0",
    "optifine-utils": "^1.5.4",
    "promise-mysql": "^5.2.0",
    "skinview3d": "^3.0.0-alpha.1",
    "toml": "^3.0.0",
    "uaup-js": "^0.0.22",
    "unzip": "^0.1.11",
    "wordpress": "^1.4.1",
    "wpapi": "^1.2.2"
  },
  "bugs": {
    "url": "https://github.com/ItsRubenete/secrecylauncher/issues"
  },
  "homepage": "https://github.com/ItsRubenete/secrecylauncher#readme"
}
